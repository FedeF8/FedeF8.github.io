---
permalink: /feed.json
---

{
  "version": "https://jsonfeed.org/version/1",
  "title": {{ site.title | jsonify }},
  "home_page_url": "{{ site.url }}{{ site.baseurl }}",
  "feed_url": "{{ site.url }}{{ site.baseurl }}/feed.json",
  "description": {{ site.description | jsonify }},
  "items": [
    {% for post in site.posts %}
    {
      "id": "{{ post.id }}",
      "url": "{{ site.url }}{{ site.baseurl }}{{ post.url }}",
      "title": {{ post.title | jsonify }},
      "content_html": {{ post.content | jsonify }},
      "cientific": {{ post.cientific | jsonify }},
      "common": {{ post.common | jsonify }},
      "description": {{ post.description | jsonify }},
      "date_published": "{{ post.date | date_to_rfc3339 }}",
      "tags": [
        {% for tag in post.tags %}
        "{{ tag }}"
        {% unless forloop.last %},{% endunless %}
        {% endfor %}
      ]
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
